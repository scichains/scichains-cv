      ],
      "technology": "jvm",
      "language": "java",
      "folders": {
        "models": "java/models/opencv",
        "resources": "java/resources"
      },
      "configuration": {
        "require_existing_paths": true,
        "classpath": [
          "java/lib/javacpp/javacpp-${javacpp.version}.jar",
