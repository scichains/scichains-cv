{
  "app": "executors-extension",
  "platforms": [
    {
      "id": "7c87e960-19b7-40d3-bc8b-d904228c23d3",
      "name": "CV-AlgART",
      "category": "cv",
      "description": "Computer vision base functions, AlgART",
      "tags": [
        "$public"
      ],
      "technology": "jvm",
      "language": "java",
      "folders": {
        "models": "java/models/algart",
        "resources": "java/resources"
      },
      "configuration": {
        "classpath": [
          "java/lib/${artifactId}-${version}.jar",
          "java/lib/${artifactId}-${version}-tests.jar"
        ]
      }
    },
    {
      "id": "97fc70a0-3e99-444b-908b-50bc9fda8de1",
